import { Output } from "./Output.ts.old";


export class SmartAddress {
    outputManager: Output;
    programmerVal: number;
    playbackVal: number;
    
    constructor(outputManager: Output) {
        this.outputManager = outputManager;
    }

    programmerSays(val: number| undefined) {
        this.programmerVal = val;
    }

    playbackSays(val: number | undefined) {
        this.playbackVal = val;
    }
}