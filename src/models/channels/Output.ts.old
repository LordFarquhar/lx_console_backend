import { SmartAddress } from "./SmartAddress.ts.old";

export class Output {
	smartAddresses: Map<number, SmartAddress>;
    output: Buffer;

	constructor() {
        this.output = Buffer.alloc(512);
		this.smartAddresses = new Map(
			new Array(255).fill(undefined).map((elt, i) => [i+1, new SmartAddress(this)])
		);

	}

    setOutputVal(i: number, v: number) {
        this.output[i-1] = v;
    }
}
